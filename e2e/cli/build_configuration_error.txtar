! exec $OPACTL build --config config.d/bundle.yml --data-dir tmp
stderr '^load config failed: upsert bundle "hello-world" failed'
! stdout .

! exec $OPACTL build --config config.d/bundle.yml --data-dir tmp --non-interactive
